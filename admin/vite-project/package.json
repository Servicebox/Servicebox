{
  "name": "vite-project",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "host": "vite",
    "dev": "vite --host --port 3001",
    "build": "vite build",
    "lint": "eslint . --ext js,jsx --report-unused-disable-directives --max-warnings 0",
    "preview": "vite preview"
  },
  "extends": [
    "eslint:recommended",
    "plugin:react/recommended",
    "plugin:react-hooks/recommended"
  ],
  "dependencies": {
    "dotenv": "^16.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^6.23.1"
  },
  "compilerOptions": {
    "types": [
      "vite/client"
    ]
  },
  "devDependencies": {
    "@types/react": "^18.2.66",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^4.2.1",
    "eslint": "^8.57.0",
    "eslint-plugin-react": "^7.34.1",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.6",
    "vite": "^5.2.0"
  },
  "baseUrl": "./src",
  "paths": {
    "@assets/*": [
      "assets/* "
    ],
    "@components/*": [
      "components/*"
    ],
    "@features/*": [
      "features/*"
    ],
    "@pages/*": [
      "pages/*"
    ],
    "@theme/*": [
      "@theme/*"
    ],
    "@utils/*": [
      "@utils/*"
    ]
  }
}
