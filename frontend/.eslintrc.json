{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "eslint:recommended",
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "off",
    "no-dupe-keys": "off",
    "no-useless-escape": "off",
    "no-undef": "off"
  },
  "globals": {
    "$": "readonly",
    "$$": "readonly"
  }
}